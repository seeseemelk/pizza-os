PROJECT_NAME = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER = @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@
STRIP_FROM_PATH = @PROJECT_SOURCE_DIR@/src
INPUT = @PROJECT_SOURCE_DIR@/src/kernel \
        @PROJECT_SOURCE_DIR@/src/libc \
FILE_PATTERNS = *.h
RECURSIVE = YES
USE_MDFILE_AS_MAINPAGE = @PROJECT_SOURCE_DIR@/src/kernel/mainpage.md
JAVADOC_AUTOBRIEF = YES
